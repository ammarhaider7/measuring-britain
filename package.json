{
  "name": "webpack-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run lint && npm run build && node ./server/bin/www",
    "launch": "npm run build && node ./server/bin/www",
    "stats": "webpack --config ./config/webpack.prod.config.js --profile --json > stats.json -p && node ./server/bin/www",
    "watch": "run-p start-wds start-express",
    "build": "webpack --config ./config/webpack.prod.config.js --progress --profile --colors -p",
    "start-wds": "node ./server/wds.js",
    "start-express": "nodemon -e jsx --watch server/views --watch server/routes ./server/bin/dev",
    "lint": "eslint src && eslint server"
  },
  "dependencies": {
    "babel-register": "^6.9.0",
    "body-parser": "~1.13.2",
    "bootstrap-sass": "^3.3.7",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "express-react-views": "^0.10.2",
    "jquery": "^3.1.1",
    "morgan": "~1.6.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "redux": "^3.6.0",
    "rxjs": "^5.0.0-rc.5",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.16.0",
    "clean-webpack-plugin": "^0.1.10",
    "coffee-jsx-loader": "^0.1.5",
    "coffee-loader": "^0.7.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.23.1",
    "eslint": "^3.11.1",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "image-webpack-loader": "^2.0.0",
    "node-sass": "^3.8.0",
    "nodemon": "^1.9.2",
    "npm-run-all": "^3.1.2",
    "proxy-middleware": "^0.15.0",
    "purecss": "^0.6.0",
    "purifycss-webpack-plugin": "^2.0.3",
    "redux-devtools": "^3.3.1",
    "redux-devtools-extension": "^1.0.0",
    "reload": "^1.1.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "2.1.0-beta.9"
  }
}
